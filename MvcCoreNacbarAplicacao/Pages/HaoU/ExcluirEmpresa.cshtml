@page
@model EmpresaExcluirModel
@{
    ViewData["Title"] = "NsApp - Excluir Empresa";
}

<h2>Deseja excluir a empresa @Model.Empresa.DsNomeempresa?</h2>
<form method="post">
    <button type="submit" asp-route-id="@Model.Empresa.IdEmpresa" class="btn btn-primary">Excluir</button>
    <a asp-page="EmpresaLeitura" class="btn btn-primary">Retornar</a>
</form>


@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}