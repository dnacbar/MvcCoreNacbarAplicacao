@page
@model UFExcluirModel
@{
    ViewData["Title"] = "NsApp";
}

<h2>DESEJA EXCLUIR O MUNICÍPIO @Model.Municipio.DsMunicipio.ToUpper()?</h2>
<form method="post">
    <button type="submit" asp-route-id="@Model.Municipio.IdMunicipio" class="btn btn-primary">EXCLUIR</button>
    <a href="/PageUF/UFLeitura?EnumUf=MUNICIPIO" class="btn btn-primary">RETORNAR</a>
</form>


@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}